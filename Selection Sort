	void selectionSort(int arr[], int n)
	{
	    //code here
	    for(int i=0; i<n-1; i++){
	        int index = i;
	        for(int j=i; j<=n-1; j++){
	            if(arr[index]>arr[j]){
	                index = j;
	            }
	        }     
	            int temp = arr[index];
	            arr[index] = arr[i];
	            arr[i] = temp;
	        
	    }
	}
}
